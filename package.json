{
  "name": "mern-stack-boilerplate",
  "version": "0.0.0",
  "private": true,
  "main": "server.js",
  "scripts": {
    "server": "node server.js",
    "client": "cd client && npm start",
    "start": "./node_modules/.bin/concurrently \"node server.js\" \"cd client && npm start\"",
    "build": "cd client && npm build",
    "deploy": "npm build && git add . && git commit -m \"Building for production\" && git push heroku master && git push origin master"
  },
  "dependencies": {
    "body-parser": "^1.17.2",
    "concurrently": "^3.5.0",
    "cookie-parser": "^1.4.3",
    "debug": "^2.6.8",
    "express": "^4.15.4",
    "jade": "^1.11.0",
    "mongoose": "^4.11.9",
    "morgan": "^1.8.2",
    "serve-favicon": "^2.4.3"
  }
}
